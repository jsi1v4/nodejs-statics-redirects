{
  "name": "nodejs-statics-redirects",
  "version": "1.0.0",
  "description": "Application redirects from url to static page or link.",
  "author": "Jsi1v4",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "nodemon",
    "watch": "NODE_ENV=development node -r ts-node/register ./src/index.ts",
    "test": "NODE_ENV=development jest",
    "build": "webpack",
    "build:prod": "webpack --mode production",
    "deploy": "npm run deploy:clear && npm run build:prod",
    "deploy:clear": "rm -r dist/ && mkdir dist/ && mkdir dist/statics"
  },
  "dependencies": {
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/helmet": "^0.0.48",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/supertest": "^2.0.10",
    "copy-webpack-plugin": "^6.2.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "pm2": "^4.4.1",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  }
}
